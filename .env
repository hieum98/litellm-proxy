# ================================================
# LITELLM PROXY CONFIGURATION
# ================================================

# Master Keys
LITELLM_MASTER_KEY=sk-your-very-secure-master-key-here
LITELLM_SALT_KEY=sk-your-very-secure-salt-key-here

# ================================================
# REDIS CONFIGURATION
# ================================================
# For Docker: use "redis" (service name)
# For direct startup: use "localhost"
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=redis_secure_password_change_me  # Disabled - no password authentication

# ================================================
# LLM PROVIDER API KEYS
# ================================================

# OpenAI
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_API_KEY_BACKUP=sk-your-backup-openai-key-here

# Anthropic Claude
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Google Gemini
GOOGLE_API_KEY=your-google-api-key-here
